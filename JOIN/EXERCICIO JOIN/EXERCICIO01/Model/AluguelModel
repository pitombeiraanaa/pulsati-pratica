package com.example.exemploJoin.Model;


/*
CREATE TABLE aluguel_ana (
  id NUMBER GENERATED BY DEFAULT AS IDENTITY,
  id_livro NUMBER NOT NULL,
  data_aluguel DATE NOT NULL,
  PRIMARY KEY (id),
  CONSTRAINT aluguel_livro_fk FOREIGN KEY (id_livro)
   REFERENCES livro (id)
);
 */
import jakarta.persistence.*;
import lombok.Data;

import java.util.Date;

@Entity
@Table(name = "aluguel_ana")
@Data
public class AluguelModel {

    @Id

    private Long id;

    @Column(name = "data_aluguel")
    private Date dataAluguel;


    @ManyToOne
    @JoinColumn(name = "id_livro", referencedColumnName = "id")
    private LivroModel livro;

}
