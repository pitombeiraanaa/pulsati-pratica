package com.example.exemploJoin.Model;
/*
CREATE TABLE vendas_lucas (
  id NUMBER GENERATED BY DEFAULT AS IDENTITY,
  nome_comprador VARCHAR2(50) NOT NULL,
  id_pessoa NUMBER NOT NULL,
  CONSTRAINT venda_fk FOREIGN KEY (id_pessoa) REFERENCES pessoa (id),
    PRIMARY KEY (id)
);
 */
import jakarta.persistence.*;
import lombok.Data;

@Entity
@Table(name = "vendas_lucas")
@Data
public class VendasModel {
    @Id
    @Column(name = "id")
    private Long id;

    @Column(name = "nome_comprador")
    private String nomeComprador;

    @ManyToOne
    @JoinColumn(name = "id_pessoa", referencedColumnName = "id")
    private PessoaModel id_pessoa;


}
