public class Endereco {
    private String rua;
    private int numero;
    private int cep;
    

    Endereco(String rua , int numero , int cep){}
    Endereco (){}
    public String getRua() {
        return rua;
    }

    public void setRua(String rua) {
        this.rua = rua;
    }

    public int getNumero() {
        return numero;
    }

    public void setNumero(int numero) {
        this.numero = numero;
    }

    public int getCep() {
        return cep;
    }

    public void setCep(int cep) {
        this.cep = cep;
    }
}
